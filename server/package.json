{
    "name": "pagecall-demo-server",
    "version": "1.0.0",
    "description": "PageCall demo node.js server",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "nodemon",
        "start:prod": "tsc && pm2 startOrReload pm2.config.js",
        "stop": "pm2 stop all && pm2 kill",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint --fix src/**/*.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pplink/pagecall-demo.git"
    },
    "author": "PPLINK.INC",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/pplink/pagecall-demo/issues"
    },
    "homepage": "https://github.com/pplink/pagecall-demo#readme",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{ts}": [
            "eslint --fix"
        ]
    },
    "devDependencies": {
        "@types/node": "^15.14.3",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^4.3.8",
        "lint-staged": "^10.2.11",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "ts-node": "^9.1.1",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-useragent": "^1.0.15",
        "helmet": "^4.6.0",
        "morgan": "^1.10.0",
        "pm2": "^5.1.0",
        "randomstring": "^1.2.1"
    }
}
